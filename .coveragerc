[run]
source = ["src/dossier"]
branch = true
omit = [
    "*/tests/*",
    "*/__pycache__/*",
]

[report]
exclude_lines = [
    "pragma: no cover",
    "def __repr__",
    "raise AssertionError",
    "raise NotImplementedError",
    "if __name__ == .__main__.:",
    "if TYPE_CHECKING:",
]
show_missing = true
skip_covered = false
